            <COMMAND_ID>SHOW</COMMAND_ID>
            <TOOLTIP_HELP>Connect to the Selected AVR Programmer</TOOLTIP_HELP>
         </SHOW>
         <FPROG>
            <COMMAND_ID>FPROG</COMMAND_ID>
            <TOOLTIP_HELP>Write Flash Memory Using Current Settings</TOOLTIP_HELP>
         </FPROG>
         <EPROG>
            <COMMAND_ID>EPROG</COMMAND_ID>
            <TOOLTIP_HELP>Write EEPROM Memory Using Current Settings</TOOLTIP_HELP>
         </EPROG>
         <EREAD>
            <COMMAND_ID>EREAD</COMMAND_ID>
            <TOOLTIP_HELP>Read EEPROM Memory Using Current Settings</TOOLTIP_HELP>
         </EREAD>
         <AUTO>
            <COMMAND_ID>AUTO</COMMAND_ID>
            <TOOLTIP_HELP>Start Auto Programming</TOOLTIP_HELP>
         </AUTO>
      </STK500>
   </TOOLBARS>
   <SHORTCUTS>
      <NMB_SHORTCUTS>1</NMB_SHORTCUTS>

<GUI_PLUGIN>
   <FRAMEWORK>
      <NAME>STK500</NAME>
      <COMMAND_LIST>[SHOW:FPROG:EPROG:EREAD:AUTO:CONNECT]</COMMAND_LIST>
      <HELP_FILE/>
   </FRAMEWORK>
   <MENUS>
      <MENU_NAMES>[TOOLS]</MENU_NAMES>
      <TOOLS>
         <MENU_TEXT>Program AVR</MENU_TEXT>
         <SUB_MENU>TRUE</SUB_MENU>
         <TOOLTIP_HELP/>
         <MENU_NAMES>[CONNECT:SHOW:-:FPROG:EPROG:EREAD:AUTO]</MENU_NAMES>
         <CONNECT>
            <MENU_TEXT>Connect...</MENU_TEXT>
            <SUB_MENU>FALSE</SUB_MENU>
            <TOOLTIP_HELP>Display the 'Connect' Dialog</TOOLTIP_HELP>
            <COMMAND_ID>CONNECT</COMMAND_ID>
         </CONNECT>
         <SHOW>
            <MENU_TEXT>Auto Connect</MENU_TEXT>
            <SUB_MENU>FALSE</SUB_MENU>
            <TOOLTIP_HELP>Connect to the Selected AVR Programmer</TOOLTIP_HELP>
            <COMMAND_ID>SHOW</COMMAND_ID>
         </SHOW>
         <FPROG>
            <MENU_TEXT>Write Flash</MENU_TEXT>
